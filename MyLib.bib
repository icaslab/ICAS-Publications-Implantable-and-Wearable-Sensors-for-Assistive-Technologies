	file = {IEEE Xplore Full Text PDF:C\:\\Users\\A_Cha\\Zotero\\storage\\STRQPSEF\\Kim et al. - 2020 - 3-D Space Visualization System Using Ultrasonic Se.pdf:application/pdf;IEEE Xplore Abstract Record:C\:\\Users\\A_Cha\\Zotero\\storage\\MHMMD9P5\\9034070.html:text/html}
