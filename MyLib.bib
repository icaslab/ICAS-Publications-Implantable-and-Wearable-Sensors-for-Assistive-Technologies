	keywords = {pressure control, pressure sensors, electric sensing devices, Sensors, pressure detection, digital signals, electrical sensors, flexible electronics, flexible polymers, flexible sensor, flexible strain-pressure sensors, integrated wearable flexible sensors, MEMS device architecture, Microelectromechanical system, Micromechanical devices, microsensors, Microstructure, PDMS, polydimethylsiloxane, polyimide-based MEMS, polymer materials, polymer MEMS, polymer microelectromechanical system, polymers, Polymers, pressure sensing, Resists, size 50 mm, Strain, strain sensing, strain sensors, stretchable polymers, Substrates, wearable technology},
	pages = {443--450}
}

@article{weathersby_thin_2019,
	title = {Thin {Magnetically} {Permeable} {Targets} for {Inductive} {Sensing}: {Application} to {Limb} {Prosthetics}},
