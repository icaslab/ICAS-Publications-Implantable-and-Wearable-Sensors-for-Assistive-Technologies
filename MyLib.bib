@article{wade_feasibility_2019,
	title = {Feasibility of {Automated} {Mobility} {Assessment} of {Older} {Adults} via an {Instrumented} {Cane}},
	volume = {23},
	issn = {2168-2208},
	doi = {10.1109/JBHI.2018.2873991},
	abstract = {This study explored the feasibility of automated characterization of functional mobility via an Instrumented Cane System (ICS) within an older adult sample of cane users. An off-the-shelf offset cane was instrumented with inertial, force, and ultrasound sensors for noninvasive data collection. Eighteen patients from a neurological out-patient rehabilitation clinic and nine independently mobile controls participated in standard clinical evaluations of mobility using the ICS while under the care of an attending physical therapist. Feasibility of the ICS was gauged through two studies. The first demonstrated the capability of the ICS to reliably collect meaningful usage metrics, and the second provided preliminary support for the discriminability of high and low falls risk from system-reported metrics. Specifically, the cane significantly differentiated patients and controls (p {\textless}; 0.05), and a measure of the variation in rotational velocity was associated with total scores on the Functional Gait Assessment (partial r = 0.61, p {\textless}; 0.01). These findings may ultimately serve to complement and even extend current clinical assessment practices.},
	number = {4},
	journal = {IEEE Journal of Biomedical and Health Informatics},
	author = {Wade, Joshua W. and Boyles, Robert and Flemming, Patricia and Sarkar, Arpan and de Riesthal, Michael and Withrow, Thomas J. and Sarkar, Nilanjan},
	month = jul,
	year = {2019},
	note = {Conference Name: IEEE Journal of Biomedical and Health Informatics},
	keywords = {handicapped aids, medical computing, patient rehabilitation, neurophysiology, geriatrics, Sensors, gait analysis, Legged locomotion, biomedical measurement, Integrated circuits, ultrasound sensors, Biomedical measurement, patient diagnosis, attending physical therapist, automated characterization, automated mobility Assessment, cane users, current clinical assessment practices, falls risk, functional gait assessment, functional mobility, ICS, independently mobile controls, inertial force, instrumented cane, instrumented cane system, Instruments, noninvasive data collection, older adult sample, older adults, Older adults, out-patient rehabilitation clinic, pervasive health, preliminary support, system-reported metrics, Ultrasonic imaging, usage metrics},
	pages = {1631--1638}
